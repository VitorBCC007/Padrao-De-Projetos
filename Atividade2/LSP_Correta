class Voador:
    def voar(self):
        pass

class Pato(Voador):
    def voar(self):
        print("Voando")

class PatoBorracha:
    pass  #A classe PatoBorracha não é forçada a fornecer uma implementação para o método voar
def executar_voar(voador):
    voador.voar()

pato = Pato()
executar_voar(pato)
