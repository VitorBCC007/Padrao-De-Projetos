class Impressora:
    def imprimir(self, documento):
        pass

class ImpressoraLaser(Impressora):
    def imprimir(self, documento):
        print(f"Imprimindo {documento} via laser")

class Scanner(Impressora):
    def imprimir(self, documento):
        print(f"Escaneando {documento}")

impressora = Scanner()
impressora.imprimir("Documento")
#A classe scanner está sendo forçada a imprimir, sem ter necessidade devido a interface impressoral
